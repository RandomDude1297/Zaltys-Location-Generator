<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3C3C3C"
    tools:context=".MainActivity">
    <EditText
        android:id="@+id/northwest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:inputType="textPersonName"
        android:textColor="#bbbbbb"
        android:textColorHint="#AAAAAAAA"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label1" />

    <TextView
        android:id="@+id/label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="West"
        app:layout_constraintStart_toStartOf="@+id/west"
        app:layout_constraintTop_toBottomOf="@+id/southwest"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/southwest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:hint="lakes:Lake Hylia"
        android:inputType="textPersonName"
        android:textColor="#bbbbbb"
        android:textColorHint="#AAAAAAAA"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label7" />

    <TextView
        android:id="@+id/label7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Southwest"
        app:layout_constraintStart_toStartOf="@+id/southwest"
        app:layout_constraintTop_toBottomOf="@+id/south"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/south"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:textColorHint="#AAAAAAAA"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label5"
 		android:textColor="#bbbbbb" />

    <TextView
        android:id="@+id/label5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="South"
        app:layout_constraintStart_toStartOf="@+id/south"
        app:layout_constraintTop_toBottomOf="@+id/southeast"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/southeast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:hint="villages:Kokiri Forest"
        android:inputType="textPersonName"
        android:textColor="#bbbbbb"
        android:textColorHint="#AAAAAAAA"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label4" />

    <TextView
        android:id="@+id/label4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Southeast"
        app:layout_constraintStart_toStartOf="@+id/southeast"
        app:layout_constraintTop_toBottomOf="@+id/east"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/east"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
