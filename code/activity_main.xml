        android:id="@+id/label3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="East"
        app:layout_constraintStart_toStartOf="@+id/east"
        app:layout_constraintTop_toBottomOf="@+id/northeast"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/northeast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:hint="villages:Kakariko"
        android:inputType="textPersonName"
        android:textColor="#bbbbbb"
        android:textColorHint="#AAAAAAAA"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label2" />

    <TextView
        android:id="@+id/label2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Northeast"
        app:layout_constraintStart_toStartOf="@+id/northeast"
        app:layout_constraintTop_toBottomOf="@+id/north"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/north"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:hint="towns:Castle Town"
        android:inputType="textPersonName"
        android:textColor="#bbbbbb"
        android:textColorHint="#AAAAAAAA"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label8" />

    <TextView
        android:id="@+id/label8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="North"
        app:layout_constraintStart_toStartOf="@+id/north"
        app:layout_constraintTop_toBottomOf="@+id/northwest"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/west"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:hint="valleys:Gerudo Valley"
        android:inputType="textPersonName"
        android:textColor="#bbbbbb"
        android:textColorHint="#AAAAAAAA"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label" />

    <TextView
        android:id="@+id/label1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Northwest"
        android:textColor="#bbbbbb"
        app:layout_constraintStart_toStartOf="@+id/northwest"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/threats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
        android:hint="Stalchildren &lt;at_night&gt;"
        android:inputType="textPersonName"
        android:textColor="#bbbbbb"
        android:textColorHint="#AAAAAAAA"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Threats"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/life"
 		android:textColor="#bbbbbb" />

    <EditText
        android:id="@+id/life"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:ems="9"
